{
    "containerDefinitions": [
        {
            "name": "imgai-backend",
            "image": "947106399822.dkr.ecr.eu-west-3.amazonaws.com/imgai",
            "cpu": 1,
            "portMappings": [
                {
                   "containerPort": 7000
                }
            ],
            "essential": true,
            "secrets": [
                {
                    "name": "SD_API_KEY",
                    "valueFrom": "arn:aws:ssm:eu-west-3:947106399822:parameter/SD_API_KEY"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "secretOptions": null,
                "options": {
                    "awslogs-group": "/ecs/imgai_backend",
                    "awslogs-region": "eu-north-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "test",
    "executionRoleArn": "arn:aws:iam::947106399822:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "tags": []
}

